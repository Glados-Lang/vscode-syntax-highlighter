{
  "name": "xenon-syntax-highlighting",
  "displayName": "Xenon Syntax Highlighting",
  "description": "A syntax highlighter for the Xenon language.",
  "repository": {"url": "https://github.com/Xenon-Lang-Org/vscode-syntax-highlighter"},
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "xenon",
        "aliases": [
          "Xenon",
          "xenon"
        ],
        "extensions": [
          ".xn"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "xenon",
        "scopeName": "source.xn",
        "path": "./syntaxes/xenon.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "scripts": {
    "test": "vscode-test"
  }
}
