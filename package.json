{
  "name": "xenon-syntax-highlighting",
  "displayName": "xenon-syntax-highlighting",
  "description": "A syntax highlighter for the Xenon language.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "xenon",
      "aliases": ["Xenon", "xenon"],
      "extensions": [".xn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "xenon",
      "scopeName": "source.xn",
      "path": "./syntaxes/xenon.tmLanguage.json"
    }]
  }
}
